{
    "name": "testcafe-runner",
    "main": "./lib/testcafe-runner",
    "version": "0.1.0",
    "description": "A debugger package for Atom, allows to run TestCafe tests from context menu or built-in commands",
    "author": "Nara Lewinsky",
    "keywords": [
        "TestCafe",
        "test",
        "runner"
    ],
    "activationCommands": {
        "atom-workspace": [
            "testcafe-runner:runSelectedTest"
        ]
    },
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript"
    ],
    "repository": "https://github.com/atom/testcafe-runner",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {
        "jquery": "^3.4.1",
        "minimist": "^1.2.0",
        "process": "^0.11.10",
        "shelljs": "^0.8.3",
        "testcafe": "^1.3.3",
        "testcafe-browser-tools": "^1.6.5"
    },
    "devDependencies": {
        "@types/jquery": "^2.0.47",
        "@types/node": "^6.0.40",
        "typescript": "^2.0.3"
    }
}
